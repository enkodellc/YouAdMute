{
    "name": "YouMute",
    "short_name": "YouMute",
    "description": "Automatically mute and skip YouTube\u2122 video ads",
    "version": "0.2.0",
    "manifest_version": 3,
    "icons": {
        "128": "logo.png"
    },
    "content_scripts": [
        {
            "matches": [ "https://www.youtube.com/*" ],
            "js": [ "inject.js" ],
            "css": [ "inject.css" ]
        }
    ]
}